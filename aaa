#include <stdio.h>
#define SIZE 5

int x(n, b);

int main(void)
{
	int a[SIZE] = { 0, 0, 0, 0, 0 };


	x(a, 5);


	return 0;
}
int x(n, b)
{
	int a[SIZE] = { 0, 0, 0, 0, 0 };
	int i, j;
	while (1)
	{
		for (i = 0; i<5; i++)
			printf("  %d", a[i]);
		printf("\n");
		printf("번호를 눌러 자리를 선택해주세요");
		scanf("%d", &j);

		if (j <= 0 || j>5)
		{
			continue;
		}
		if (a[j - 1] == 0)
		{
			a[j - 1] = 1;
			printf("예약되었습니다.\n");
		}
		else
			printf("이미 예약된 자리입니다.\n");
		if (a[0] == 1 && a[1] == 1 && a[2] == 1 && a[3] == 1 && a[4] == 1)
			return 0;
	}
////
	

	return 0;
